<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans&display=swap" rel="stylesheet">
    @Styles.Render("~/Content/Site.css")
</head>
<body>
    <div id="cover" class="none"></div>
    <div id="bigWrapper">
        <div class="image-container"></div>
        <nav>
            <ul class="navbar-list">
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li>@Html.ActionLink("About Us", "About", "Home")</li>
                <li>@Html.ActionLink("Legal", "Legal", "Home")</li>
                <li>@Html.ActionLink("Contact Us", "Contact", "Home")</li>
                <li>@Html.ActionLink("Register", "Index", "Home")</li>
            </ul>
        </nav>
        <div class="body-content">
            <div class="two-column-container">
                @RenderBody()
                
            </div>

            <footer>
                <p>Plant Photos and Information by: MissouriPlants.com Retrevied: Feb 08 </p>
                <span>@Html.ActionLink("Contact Us", "Index", "Home")</span>
            </footer>
        </div>
        </div>
@Scripts.Render("~/bundles/imageClick")
@RenderSection("scripts", required: false)
</body>
</html>
